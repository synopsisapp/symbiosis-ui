{
  "name": "@synopsisapp/symbiosis-ui",
  "version": "0.1.28",
  "license": "MIT",
  "type": "module",
  "main": "dist/symbiosis-ui.umd.js",
  "module": "dist/symbiosis-ui.es.js",
  "types": "dist/src/index.d.ts",
  "files": ["dist", "src", "internal-assets"],
  "exports": {
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/plugin.es.js",
      "require": "./dist/plugin/plugin.umd.js"
    },
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/symbiosis-ui.es.js",
      "require": "./dist/symbiosis-ui.umd.js"
    }
  },
  "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
  "scripts": {
    "dev": "vite",
    "build": "npm run test && vite build && npm run build:plugin && tsc --emitDeclarationOnly",
    "build:plugin:types": "tsc -p tsconfig.plugin.json",
    "build:plugin": "vite build -c vite.config.plugin.ts && npm run build:plugin:types",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint:check": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format:check": "biome format .",
    "format:fix": "biome format --write .",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/lodash": "^4.17.7",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "node-html-parser": "^6.1.13",
    "react-day-picker": "^9.4.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.4.0"
  },
  "peerDependencies": {
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/react-table": "^8.20.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "framer-motion": "^11.3.31",
    "lucide-react": "^0.383.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.7",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "webpack": "^5.64.4",
    "zod": "^3.23.8"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.3.1"
  }
}
